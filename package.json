{
  "name": "hel-react-vite",
  "appGroupName": "hel-react-vite",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "unpkg": "hel_dist/index.html",
  "scripts": {
    "dev": "vite --mode development",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build": "npm run check_name && npm run build_dist && npm run build_meta && npm run build_proxy",
    "build_dist": "vite --mode production build",
    "check_name": "node ./scripts/check.mjs",
    "build_proxy": "npm run build_proxy_umd && npm run build_proxy_es && cp ./hel_proxy/entry.js ./hel_dist",
    "build_proxy_umd": "rollup -c",
    "build_proxy_es": "cross-env BUILD_ENV=es rollup -c",
    "build_cust": "npm run set_home_page \"npm run build && npm run build_meta\"",
    "set_home_page": "cross-env-shell HEL_APP_HOME_PAGE=http://127.0.0.1:9001",
    "build_meta": "node scripts/meta.mjs",
    "prepublishOnly": "node ./scripts/prepublishOnly.cjs"
  },
  "files": [
    "hel_dist",
    "hel_proxy",
    "hel_proxy_es",
    "scripts",
    "README.md",
    "src"
  ],
  "dependencies": {
    "cross-env": "^7.0.3",
    "hel-dev-utils": "^5.1.0",
    "hel-iso": "^4.4.0",
    "hel-lib-proxy": "^4.9.7",
    "i": "^0.3.7",
    "npm": "^11.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^22.13.17",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cash-cp": "^0.2.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
